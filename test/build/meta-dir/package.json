{
  "private": true,
  "version": "1.0.1",
  "name": "@hisystems/snowpack-test-meta-dir",
  "description": "Test that makes sure that the metaDir can be configured and still build",
  "scripts": {
    "prepare": "snowpack",
    "testbuild": "snowpack build"
  },
  "snowpack": {
    "mount": {
      "./src": "/_dist_"
    },
    "buildOptions": {
      "metaDir": "/static/snowpack",
      "minify": false
    }
  },
  "dependencies": {
    "shallow-equal": "^1.2.1"
  },
  "devDependencies": {
    "@hisystems/snowpack": "^2.7.0"
  }
}
